public class FlightClassChecksPage {
	WebDriver driver =null;
	public void the_user_click_on_their_of_choice_class_and_select(String string) {

		driver.findElement(By.xpath("/html/body/main/div[2]/div[2]/div/div[1]/div/div/div/div[3]/div/div/form/div/div[2]/div/div[1]/div[4]/div[1]/div[2]/div/span[2]")).click();
	}

}